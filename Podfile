target "Ontario Ski Report" do
    platform :ios, '9.0'
	use_frameworks!
	pod 'GoogleMaps'
	pod 'Firebase/Core'
	pod 'JGActionSheet'
	pod 'SwiftSoup'
end

target "Today's Ski Report" do
	platform :ios, '9.0'
    use_frameworks!
	pod 'SwiftSoup'
end

target "Ontario Ski Report For Apple Watch Extension" do
	platform :watchos, '3.0'
	use_frameworks!
	pod 'SwiftSoup'
end
